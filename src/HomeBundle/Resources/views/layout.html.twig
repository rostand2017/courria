    <script>

    //show shopping cart panel
    $('.js-show-cart').on('click', function (e) {
        panel = $('#cart_list');
        url = $(this).data("url");
        $.ajax({
            type:"post",
            url: url,
            dataType: "json",
            success: function (data) {
                if(data.status == 1){
                    panel.html("<li class='text-center'>"+data.mes+"</li>");
                    $('#totalCart').text("Total: --- XAF");
                }else{
                    panel.html(data.mes);
                    $('#totalCart').text("Total: "+data.total+" XAF");
                }
            },
            beforeSend: function () {
                panel.html("" +
                    "<li class='text-center'>Chargement...</li>" +
                    "");
            },
            error: function () {
                panel.html("" +
                    "<li class='text-center'>Une erreur est survenue</li>" +
                    "");
            }

        })
    });


    $(".js-select2").each(function(){
        $(this).select2({
            minimumResultsForSearch: 20,
            dropdownParent: $(this).next('.dropDownSelect2')
        });
    })
    </script>
